{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "assets": [{
    "include": "../*.yml",
    "outDir": "./dist"
  }],
  "compilerOptions": {
    "assets": [
      {
        "include": "../config/*.yaml",
        "outDir": "./dist/config"
      }
    ]
  }
}
